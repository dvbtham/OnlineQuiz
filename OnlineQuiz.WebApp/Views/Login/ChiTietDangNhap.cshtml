
@{
    ViewBag.Title = "ChiTietDangNhap";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<button id="suathongtin"><i class="glyphicon glyphicon-cog"></i></button>
<div class="container chitietdangnhap">

    <div class="col-md-6">
        <h3>Thông tin thí sinh:</h3>
        <form>
            <div class="form-group">
                <label>ID dự thi:</label>
                <input type="text" name="name" value="323" />
            </div>
            <div class="form-group">
                <label>Họ và tên:</label>
                <input type="text" name="name" value="ththth" />
            </div>
            <div class="form-group">
                <label>Ngày sinh:</label>
                <input type="text" name="name" value="" />
            </div>
            <div class="form-group">
                <label>Giới tính:</label>
                <input type="text" name="name" value="" />
            </div>
            <div class="form-group">
                <label>Chứng minh nhân dân:</label>
                <input type="text" name="name" value="" />
            </div>
        </form>
    </div>
    <div class="col-md-6">
        <h3>Thông tin hội đồng thi:</h3>
        <form>
            <div class="form-group">
                <label>Hội đồng thi:</label>
                <input type="text" name="name" value="Trường Đại học Đông Á" />
            </div>
            <div class="form-group">
                <label>Tên tín chỉ:</label>
                <select>
                    <option value="CoBan">Cơ Bản</option>
                    <option value="NangCao">Năng Cao</option>
                </select>
            </div>
            <div class="form-group">
                <label>Tên mô đun dự thi:</label>
                <input type="text" placeholder="(Nếu thi nâng cao)" name="name" value="" />
            </div>
            <div class="form-group">
                <label>Ngày Thi:</label>
                <input type="text" name="name" value="" />
            </div>
            <div class="form-group">
                <label>Xuất thi:</label>
                <input type="text" name="name" value="" />
            </div>
        </form>
        <button>Vào Thi >></button>
    </div>
</div>

